<c:FragmentDefinition
	xmlns="sap.m"
	xmlns:c="sap.ui.core"
    xmlns:l="sap.ui.layout"
    xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">

    <Panel backgroundDesign="Transparent" visible="{DataModel>/NewPayModeButton/QuietanzanteMaskVisible}" headerText="Inserisci quietanzante" >
      <l:Grid defaultSpan="XL12 L12 M12 S12" vSpacing="0.5">
        <Label text="{i18n>labelCognome}" class="sapUiTinyMarginTop sapUiLargeMarginEnd" required="true" wrapping="true">  
          <layoutData><l:GridData  span="XL1 L2 M6 S12" /></layoutData>
        </Label>
        <Input  value="{DataModel>/NewModalitaPagamentoEntity/QCognome}">
          <layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
        </Input>
        <Label text="{i18n>labelNome}" class="sapUiTinyMarginTop sapUiLargeMarginEnd" required="true" wrapping="true">  
          <layoutData><l:GridData  span="XL1 L2 M6 S12" /></layoutData>
        </Label>
        <Input  value="{DataModel>/NewModalitaPagamentoEntity/QNome}">
          <layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
        </Input>
        <Label text="{i18n>labelCodiceFiscale}" class="sapUiTinyMarginTop sapUiLargeMarginEnd" wrapping="true">  
          <layoutData><l:GridData  span="XL1 L2 M6 S12" /></layoutData>
        </Label>
        <Input value="{DataModel>/NewModalitaPagamentoEntity/QCodiceFiscale}" >
          <layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
        </Input>
        <Label text="{i18n>labelQualifica}" class="sapUiTinyMarginTop sapUiLargeMarginEnd" wrapping="true">  
          <layoutData><l:GridData  span="XL1 L2 M6 S12" /></layoutData>
        </Label>
        <Input  value="{DataModel>/NewModalitaPagamentoEntity/QQualifica}">
          <layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
        </Input>

        <Label text="{i18n>labelDataNascita}" class="sapUiTinyMarginTop sapUiLargeMarginEnd" wrapping="true">  
          <layoutData><l:GridData  span="XL1 L2 M6 S12" linebreak="true"/></layoutData>
        </Label>
        <DatePicker							     
            class="sapUiTinyMarginBottom"	
            valueFormat="dd.MM.yyyy"
            displayFormat="dd.MM.yyyy"
            value="{
                path: 'DataModel>/NewModalitaPagamentoEntity/QDataNascita',
                type: 'sap.ui.model.type.Date',
                formatOptions: {
                    pattern: 'dd.MM.yyyy'
                    }
                }"
            >	          
            <layoutData><l:GridData span="XL2 L2 M6 S12" /></layoutData>                
        </DatePicker>
        <Label text="{i18n>labelLuogoNascita}" class="sapUiTinyMarginTop sapUiLargeMarginEnd" wrapping="true">  
          <layoutData><l:GridData  span="XL1 L2 M6 S12" /></layoutData>
        </Label>
        <Input value="{DataModel>/NewModalitaPagamentoEntity/QLuogoNascita}" >
          <layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
        </Input>
        <Label text="{i18n>labelProvincia}" class="sapUiTinyMarginTop sapUiLargeMarginEnd" wrapping="true">  
          <layoutData><l:GridData  span="XL1 L2 M6 S12" /></layoutData>
        </Label>
        <Input value="{DataModel>/NewModalitaPagamentoEntity/QProvinciaNascita}">
          <layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
        </Input>
        <Label text="{i18n>labelTelefono}" class="sapUiTinyMarginTop sapUiLargeMarginEnd" wrapping="true">  
          <layoutData><l:GridData  span="XL1 L2 M6 S12" /></layoutData>
        </Label>
        <Input value="{DataModel>/NewModalitaPagamentoEntity/QTelefono}">
          <layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
        </Input>
        <Label text="{i18n>labelIndirizzo}" class="sapUiTinyMarginTop sapUiLargeMarginEnd" wrapping="true">  
          <layoutData><l:GridData  span="XL1 L2 M6 S12" linebreak="true"/></layoutData>
        </Label>
        <Input value="{DataModel>/NewModalitaPagamentoEntity/QIndirizzo}">
          <layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
        </Input>
        <Label text="{i18n>labelCitta}" class="sapUiTinyMarginTop sapUiLargeMarginEnd" wrapping="true">  
          <layoutData><l:GridData  span="XL1 L2 M6 S12" /></layoutData>
        </Label>
        <Input value="{DataModel>/NewModalitaPagamentoEntity/QCitta}">
          <layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
        </Input>
        <Label text="{i18n>labelCap}" class="sapUiTinyMarginTop sapUiLargeMarginEnd" wrapping="true">  
          <layoutData><l:GridData  span="XL1 L2 M6 S12" /></layoutData>
        </Label>
        <Input value="{DataModel>/NewModalitaPagamentoEntity/QCap}">
          <layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
        </Input>
        <Label text="{i18n>labelProvinciaResidenza}" class="sapUiTinyMarginTop sapUiLargeMarginEnd" wrapping="true">  
          <layoutData><l:GridData  span="XL1 L2 M6 S12" /></layoutData>
        </Label>
        <Input value="{DataModel>/NewModalitaPagamentoEntity/QProvinciaResidenza}">
          <layoutData><l:GridData  span="XL2 L2 M6 S12" /></layoutData>
        </Input>
      </l:Grid>
    </Panel>  


</c:FragmentDefinition>