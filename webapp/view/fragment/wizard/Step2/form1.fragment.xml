<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core"
	xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
>
	<!-- expandable="true" -->
	<Panel
		width="auto"
		class="margin0 padding0 expanded"
	>
		<headerToolbar>
			<OverflowToolbar style="Clear">
				<Title text="{i18n>wizardStep2SubTitle1}"/>
				<ToolbarSpacer/>
				<!-- press="newModalitaPagamentoDialogPress"
				press="newAnagraficaBeneficiarioPress" -->
				<!-- <Button   enabled="{wizardModel>/isInChange}"  text="{i18n>btnInsertPayMethod}" type="Emphasized" ></Button> -->
				<!-- <Button enabled="{wizardModel>/isInChange}"  text="{i18n>btnCreateDataBeneficary}" type="Emphasized" ></Button> -->
				<Button
					enabled="false"
					text="{i18n>btnInsertPayMethod}"
					type="Emphasized"
				/>
				<Button
					enabled="false"
					text="{i18n>btnCreateDataBeneficary}"
					type="Emphasized"
				/>
			</OverflowToolbar>
		</headerToolbar>
		<content>
			<f:SimpleForm
				id="idStep2Form1"
				editable="true"
				class="formHiddenTitle"
				layout="ResponsiveGridLayout"
				labelSpanXL="4"
				labelSpanL="3"
				labelSpanM="4"
				adjustLabelSpan="false"
				emptySpanXL="0"
				emptySpanL="4"
				columnsXL="2"
				columnsL="2"
				singleContainerFullSize="false"
			>
				<f:content>
					<Label
						design="Bold"
						text="{i18n>wizardFieldLinfnr}"
						class="sapUiSmallMarginBottom"
					/>
					<Input
						editable="{wizardModel>/isInChange}"
						id="idWizardLifnr"
						app:property="Lifnr"
						value="{wizardModel>/Lifnr}"
						submit="onSubmitLifnr"
						liveChange="onLiveChange"
					>
						<layoutData>
							<l:GridData span="XL4 L5 M6 S12"/>
						</layoutData>
					</Input>
					<Label
						design="Bold"
						text="{i18n>wizardFieldTaxnumCf}"
						class="sapUiSmallMarginBottom"
					/>
					<Input
						value="{wizardModel>/TaxnumCf}"
						editable="false"
					>
						<layoutData>
							<l:GridData span="XL4 L5 M6 S12"/>
						</layoutData>
					</Input>
					<Label
						design="Bold"
						text="{i18n>wizardFieldNameFirst}"
						class="sapUiSmallMarginBottom"
					/>
					<Input
						value="{wizardModel>/NameFirst}"
						editable="false"
					>
						<layoutData>
							<l:GridData span="XL4 L5 M6 S12"/>
						</layoutData>
					</Input>
					<Label
						design="Bold"
						text="{i18n>wizardFieldZzragSoc}"
						class="sapUiSmallMarginBottom"
					/>
					<Input
						value="{wizardModel>/ZzragSoc}"
						editable="false"
					>
						<layoutData>
							<l:GridData span="XL7 L7 M6 S12"/>
						</layoutData>
					</Input>
					<Label
						design="Bold"
						text="{i18n>wizardFieldZsede}"
						class="sapUiSmallMarginBottom"
					/>
					<Input
						value="{wizardModel>/Zsede}"
						editable="false"
					>
						<layoutData>
							<l:GridData span="XL4 L5 M6 S12"/>
						</layoutData>
					</Input>
					<Label
						design="Bold"
						text="{i18n>wizardFieldDurc}"
						class="sapUiSmallMarginBottom"
					/>
					<Input
						value="{
							path: 'wizardModel>/Zdurc',
							formatter: '.formatter.deleteFirstZeros'
						}"
						editable="false"
					>
						<layoutData>
							<l:GridData span="XL4 L5 M6 S12"/>
						</layoutData>
					</Input>
					<Label
						design="Bold"
						text="{i18n>wizardFieldScadenzaDurc}"
						class="sapUiSmallMarginBottom"
					/>
					<DatePicker
						editable="false"
						valueFormat="yyyy-MM-dd"
						displayFormat="dd.MM.yyyy"
						value="{
							path: 'wizardModel>/Zdscadenza',
							type: 'sap.ui.model.type.Date',
							formatOptions: {
								pattern: 'dd.MM.yyyy'
							}
						}"
					>
						<layoutData>
							<l:GridData span="XL4 L5 M6 S12"/>
						</layoutData>
					</DatePicker>
					<core:Title text=""/>
					<RadioButtonGroup
						id="idRadioBtn"
						class="radioBcenter"
						editable="{wizardModel>/isInChange}"
					>
						<RadioButton
							text="{i18n>wizardFieldTypeF}"
							selected="{wizardModel>/Type}"
							class="sapUiSmallMarginEnd"
						/>
						<RadioButton
							text="{i18n>wizardFieldTypeG}"
							selected="{= !${wizardModel>/Type}}"
						/>
					</RadioButtonGroup>
					<Label
						design="Bold"
						text="{i18n>wizardFieldTaxnumxl}"
						class="sapUiSmallMarginBottom"
					/>
					<Input
						value="{wizardModel>/Taxnumxl}"
						editable="false"
					>
						<layoutData>
							<l:GridData span="XL4 L5 M6 S12"/>
						</layoutData>
					</Input>
					<Label
						design="Bold"
						text="{i18n>wizardFieldNameLast}"
						class="sapUiSmallMarginBottom"
					/>
					<Input
						value="{wizardModel>/NameLast}"
						editable="false"
					>
						<layoutData>
							<l:GridData span="XL4 L5 M6 S12"/>
						</layoutData>
					</Input>
					<Label
						design="Bold"
						text="{i18n>wizardFieldTaxnumPiva}"
						class="sapUiSmallMarginBottom"
					/>
					<Input
						value="{wizardModel>/TaxnumPiva}"
						editable="false"
					>
						<layoutData>
							<l:GridData span="XL4 L5 M6 S12"/>
						</layoutData>
					</Input>
					<Label
						design="Bold"
						text="{i18n>wizardFieldZdenominazione}"
						class="sapUiSmallMarginBottom"
					/>
					<Input
						value="{wizardModel>/Zdenominazione}"
						editable="false"
					>
						<layoutData>
							<l:GridData span="XL4 L5 M6 S12"/>
						</layoutData>
					</Input>
					<Label
						design="Bold"
						text="{i18n>wizardFieldStatoDurc}"
						class="sapUiSmallMarginBottom"
					/>
					<Input
						value="{wizardModel>/Zdstatodes}"
						editable="false"
					>
						<layoutData>
							<l:GridData span="XL4 L5 M6 S12"/>
						</layoutData>
					</Input>
					<Label
						design="Bold"
						text="{i18n>wizardFieldFermoAmm}"
						class="sapUiSmallMarginBottom"
					/>
					<Input
						value="{wizardModel>/ZfermAmm}"
						editable="false"
					>
						<layoutData>
							<l:GridData span="XL4 L5 M6 S12"/>
						</layoutData>
					</Input>
				</f:content>
			</f:SimpleForm>
		</content>
	</Panel>
</core:FragmentDefinition>