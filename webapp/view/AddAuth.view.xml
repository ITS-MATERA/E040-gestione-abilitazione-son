<mvc:View
    controllerName="gestioneabilitazioneeson.controller.AddAuth"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:core="sap.ui.core"
	xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
>
    <Page  showHeader="false">
		<VBox class="bgWhite" height="100%">	

		<Title
			level="H6"
			text="{i18n>addAuthPageTitle}"
			width="100%"
			class="sapUiMediumMargin titleSize"
		>	
		
				
		</Title>

		<VBox >
			<items>
				<l:Grid  defaultSpan="XL12 L12 M12 S12" vSpacing="0">
				
				<VBox>
                    <items>
                        <FlexBox width="100%" alignItems="Center" class="labelWidth sapUiSmallMarginTop sapUiSmallMarginBottom" >
                            <items>
                                <l:Grid  defaultSpan="XL12 L12 M12 S12" vSpacing="0">	
                                    <Label  design="Bold" required="true" width="100%" text="{i18n>fieldNameGJAHR}" class="sapUiTinyMarginTop sapUiLargeMarginEnd">
                                        <layoutData><l:GridData span="XL7 L7 M4 S12" /></layoutData>
                                    </Label>
                                    <Input width="100%" id="idInputGjahr" value="{addAuhModel>/Gjahr}" submit="onSubmitGjahr" liveChange="onLiveChange" app:property="Gjahr">
                                        <layoutData><l:GridData span="XL5 L5 M7 S12"/></layoutData>
                                    </Input>
                                </l:Grid>
                            </items>     
                        </FlexBox>                                
                    </items>
                    <layoutData><l:GridData span="XL3 L4 M12 S12" linebreak="true"/></layoutData>
                </VBox>            
					<Toolbar  height="1px"> </Toolbar>
				<VBox>
                    <items>
                        <FlexBox width="100%" alignItems="Center" class="labelWidth sapUiSmallMarginTop sapUiSmallMarginBottom" >
                            <items>
                                <l:Grid  defaultSpan="XL12 L12 M12 S12" vSpacing="0">	
                                    <Label  design="Bold" required="true" width="100%" text="{i18n>columnNameZufficioCont}" class="sapUiTinyMarginTop sapUiLargeMarginEnd">
                                        <layoutData><l:GridData span="XL4 L4 M4 S12" /></layoutData>
                                    </Label>
                                    <Input width="100%" id="idInputZufficioCont" value="{addAuhModel>/ZufficioCont}" submit="onSubmitZufficioCont" liveChange="onLiveChange" app:property="ZufficioCont">
                                        <layoutData><l:GridData span="XL4 L4 M7 S12"/></layoutData>
                                    </Input>
                                </l:Grid>
                            </items>     
                        </FlexBox>                                
                    </items>
                    <layoutData><l:GridData span="XL6 L6 M12 S12"/></layoutData>
                </VBox>     
				<VBox>
                    <items>
                        <FlexBox width="100%" alignItems="Center" class="labelWidth sapUiSmallMarginTop sapUiSmallMarginBottom" >
                            <items>
                                <l:Grid  defaultSpan="XL12 L12 M12 S12" vSpacing="0">	
                                    <Label  design="Bold"  width="100%" text="{i18n>columnNameZvimDescrufficio}" class="sapUiTinyMarginTop sapUiLargeMarginEnd">
                                        <layoutData><l:GridData span="XL5 L4 M4 S12" /></layoutData>
                                    </Label>
                                    <Input width="100%" value="{addAuhModel>/ZvimDescrufficio}" editable="false">
                                        <layoutData><l:GridData span="XL7 L8 M7 S12"/></layoutData>
                                    </Input>
                                </l:Grid>
                            </items>     
                        </FlexBox>                                
                    </items>
                    <layoutData><l:GridData span="XL6 L6 M12 S12"/></layoutData>
                </VBox>   
				<VBox>
                    <items>
                        <FlexBox width="100%" alignItems="Center" class="labelWidth sapUiSmallMarginTop sapUiSmallMarginBottom" >
                            <items>
                                <l:Grid  defaultSpan="XL12 L12 M12 S12" vSpacing="0">	
                                    <Label  design="Bold" required="true" width="100%" text="{i18n>columnNameFipos}" class="sapUiTinyMarginTop sapUiLargeMarginEnd">
                                        <layoutData><l:GridData span="XL4 L4 M4 S12" /></layoutData>
                                    </Label>
                                    <Input width="100%" id="idInputFipos" value="{addAuhModel>/Fipos}" liveChange="onLiveChange" app:property="Fipos" >
                                        <layoutData><l:GridData span="XL6 L8 M7 S12"/></layoutData>
                                    </Input>
                                </l:Grid>
                            </items>     
                        </FlexBox>                                
                    </items>
                    <layoutData><l:GridData span="XL6 L6 M12 S12"/></layoutData>
                </VBox>  
				<VBox>
                    <items>
                        <FlexBox width="100%" alignItems="Center" class="labelWidth sapUiSmallMarginTop sapUiSmallMarginBottom" >
                            <items>
                                <l:Grid  defaultSpan="XL12 L12 M12 S12" vSpacing="0">	
                                    <Label  design="Bold" required="true" width="100%" text="{i18n>columnNameFistl}" class="sapUiTinyMarginTop sapUiLargeMarginEnd">
                                        <layoutData><l:GridData span="XL5 L6 M6 S12" /></layoutData>
                                    </Label>
                                    <Input width="100%" id="idInputFistl" value="{addAuhModel>/Fistl}" liveChange="onLiveChange" app:property="Fistl">
                                        <layoutData><l:GridData span="XL5 L6 M5 S12"/></layoutData>
                                    </Input>
                                </l:Grid>
                            </items>     
                        </FlexBox>                                
                    </items>
                    <layoutData><l:GridData span="XL6 L6 M12 S12"/></layoutData>
                </VBox>  
				<VBox>
					<items>
						<FlexBox width="100%" alignItems="Center" class="labelWidth sapUiSmallMarginTop sapUiSmallMarginBottom" >
							<items>
								<l:Grid  defaultSpan="XL12 L12 M12 S12" vSpacing="0">	
									<Label  design="Bold" required="true" width="100%" text="{i18n>columnNameZtipodisp3}" class="sapUiTinyMarginTop sapUiLargeMarginEnd">
										<layoutData><l:GridData span="XL4 L4 M4 S12" /></layoutData>
									</Label>
									<ComboBox
										id="idInputZtipodisp3"
										width="100%"
										change="onLiveChangeZtipodisp3List"
										items="{addAuhModel>/Ztipodisp3List}"
										>
										<core:Item  key="{addAuhModel>Ztipodisp3}" text="{addAuhModel>Zdesctipodisp3}" />
										<layoutData>
											<l:GridData span="XL8 L8 M7 S12"  />
										</layoutData>
									</ComboBox>	
								</l:Grid>
							</items>     
						</FlexBox>                                
					</items>
					<layoutData><l:GridData span="XL6 L6 M12 S12" /></layoutData>
				</VBox> 
				<VBox>
					<items>
						<FlexBox width="100%" alignItems="Center" class="labelWidth sapUiSmallMarginTop sapUiSmallMarginBottom" >
							<items>
								<l:Grid  defaultSpan="XL12 L12 M12 S12" vSpacing="0">	
									<Label  design="Bold" required="true" width="100%" text="{i18n>columnNameDatab}" class="sapUiTinyMarginTop sapUiLargeMarginEnd">
										<layoutData><l:GridData span="XL4 L4 M4 S12" /></layoutData>
									</Label>
									<DatePicker
									id="idInputDatab"
									app:property="Datab"
									change="handleChangeDatePicker"										
									valueFormat="dd-MM-yyyy"
									displayFormat="dd.MM.yyyy">
										<layoutData><l:GridData span="XL4 L8 M7 S12"/></layoutData>
									</DatePicker>
								</l:Grid>
							</items>     
						</FlexBox>                                
					</items>
					<layoutData><l:GridData span="XL6 L6 M12 S12" linebreak="true"/></layoutData>
				</VBox> 
				<VBox>
					<items>
						<FlexBox width="100%" alignItems="Center" class="labelWidth sapUiSmallMarginTop sapUiSmallMarginBottom" >
							<items>
								<l:Grid  defaultSpan="XL12 L12 M12 S12" vSpacing="0">	
									<Label  design="Bold" required="true" width="100%" text="{i18n>columnNameDatbi}" class="sapUiTinyMarginTop sapUiLargeMarginEnd">
										<layoutData><l:GridData span="XL4 L4 M4 S12" /></layoutData>
									</Label>
									<DatePicker
										id="idInputDatbi"
									app:property="Datbi"
									change="handleChangeDatePicker"										
									valueFormat="dd-MM-yyyy"
									displayFormat="dd.MM.yyyy">
										<layoutData><l:GridData span="XL4 L8 M7 S12"/></layoutData>
									</DatePicker>
								</l:Grid>
							</items>     
						</FlexBox>                                
					</items>
					<layoutData><l:GridData span="XL6 L6 M12 S12"/></layoutData>
				</VBox> 


				</l:Grid>
			</items>
			<layoutData><l:GridData  span="XL3 L4 M12 S12"/></layoutData>
		</VBox>	

	
		</VBox>
        <footer>
			<OverflowToolbar>
				<Button type="Emphasized" text="{i18n>btnBack}" press="onNavBack" >
                    <layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow" />
					</layoutData>
                </Button>
                <ToolbarSpacer />
                <Button type="Emphasized" text="{i18n>btnSave}" press="onSave" >
                    <layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow" />
					</layoutData>
                </Button>
			</OverflowToolbar>
		</footer>
    </Page>
</mvc:View>